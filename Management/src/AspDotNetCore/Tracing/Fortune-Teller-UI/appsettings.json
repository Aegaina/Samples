{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information"
    }
  },
  "spring": {
    "application": {
      "name": "fortuneUI"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldRegisterWithEureka": false
    }
  },
  "management": {
    "tracing": {
      "alwaysSample": true,
      "exporter" : {
        "zipkin" : {
          "endpoint" : "http://localhost:9411/api/v2/spans"
        }
      }
    }
  }
}
