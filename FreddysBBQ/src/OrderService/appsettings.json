{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "spring": {
    "application": {
      "name": "order-service"
    },
    "cloud": {
      "config": {
        "uri": "http://localhost:8888",
        "validateCertificates": false
      }
    }
  },
  "eureka": {
    "client": {
      "validateCertificates": false
    },
    "instance": {
      "port": 7000
    }
  },
  "mysql": {
    "client": {
      "server": "localhost"
    }
  },
  "security": {
    "oauth2": {
      "client": {
        "validateCertificates": false
      }
    }
  }
}